pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Hello this is tanmay8266 from GitHub'
            }
    }
        stage('Two'){
            steps{
            sh 'python --version'
            }
        }
        stage('Three'){
            steps{
            sh 'python3 hello_git.py'
            }
        }
}
}
